{
  "name": "api",
  "version": "0.0.1",
  "description": "api programa falla",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "postbuild": "cp -r src/templates dist/",
    "lint": "pnpm run eslint && pnpm run prettier:check",
    "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "lb-prettier \"**/*.ts\" \"**/*.js\" --list-different",
    "prettier:fix": "lb-prettier \"**/*.ts\" \"**/*.js\" --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "pnpm run eslint -- --fix",
    "pretest": "pnpm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "pnpm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && pnpm run posttest",
    "docker:build": "docker build -t api .",
    "docker:run": "docker run -p 3000:3000 -d api",
    "premigrate": "pnpm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "pnpm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "pnpm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "pnpm run clean && pnpm run build",
    "start:prod": "NODE_ENV=production pnpm start"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Ivan Mas <ivmaspre@hotmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^11.0.9",
    "@loopback/authentication-jwt": "^0.15.9",
    "@loopback/boot": "^7.0.9",
    "@loopback/core": "^6.1.6",
    "@loopback/repository": "^7.0.9",
    "@loopback/rest": "^14.0.9",
    "@loopback/rest-explorer": "^7.0.9",
    "@loopback/security": "^0.12.2",
    "@loopback/service-proxy": "^7.0.9",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.0",
    "handlebars": "^4.7.8",
    "loopback-connector-mysql": "^7.1.4",
    "puppeteer": "^24.14.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^11.0.8",
    "@loopback/eslint-config": "^15.0.4",
    "@loopback/testlab": "^7.0.8",
    "@types/bcryptjs": "^3.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.18.6",
    "eslint": "^8.57.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.2.2"
  },
  "pnpm": {
    "overrides": {
      "@loopback/security": "^0.12.2"
    }
  }
}
